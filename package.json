{
  "name": "aurelia-slickgrid",
  "version": "2.23.0",
  "description": "Slickgrid components made available in Aurelia",
  "keywords": [
    "aurelia",
    "datagrid",
    "datatable",
    "plugin",
    "slickgrid"
  ],
  "homepage": "https://github.com/ghiscoding/aurelia-slickgrid",
  "bugs": {
    "url": "https://github.com/ghiscoding/aurelia-slickgrid/issues"
  },
  "license": "MIT",
  "author": "Ghislain B.",
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/ghiscoding/aurelia-slickgrid"
  },
  "scripts": {
    "delete:dist": "cross-env rimraf dist",
    "lint": "eslint src/aurelia-slickgrid/** --ext .ts",
    "build:amd": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/amd --module amd",
    "postbuild:amd": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/amd",
    "build:commonjs": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/commonjs --module commonjs",
    "postbuild:commonjs": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/commonjs",
    "build:es2017": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/es2017 --module es2015 --target es2017",
    "postbuild:es2017": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/es2017",
    "build:es2015": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/es2015 --module es2015 --target es2015",
    "postbuild:es2015": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/es2015",
    "build:native-modules": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/native-modules --module es2015",
    "postbuild:native-modules": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/native-modules",
    "build:system": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/system --module system",
    "postbuild:system": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/system",
    "prebuild": "run-p delete:dist lint",
    "build": "run-p build:amd build:commonjs build:es2015 build:es2017 build:native-modules build:system",
    "postbuild": "npm-run-all sass:build sass:copy copy-i18n:dist copy-asset-lib",
    "build:with-e2e": "npm-run-all build cypress:ci",
    "copy-asset-lib": "cross-env copyfiles --up 2 src/assets/lib/** dist",
    "copy-i18n:dist": "cross-env copyfiles --up 3 src/assets/i18n/**/*.* dist/i18n",
    "predoc": "cross-env rimraf api.json && rimraf dist/doc-temp && tsc --project src/aurelia-slickgrid/tsconfig.build.json --outFile dist/doc-temp/aurelia-validation.js && node shape-defs && copyfiles tsconfig.json dist/doc-temp",
    "doc": "cross-env typedoc --json api.json --excludeExternals --includeDeclarations --mode modules --target ES6 --name aurelia-validation-docs dist/doc-temp/",
    "postdoc": "cross-env node shape-doc && rimraf dist/doc-temp",
    "changelog": "cross-env conventional-changelog -p angular -i CHANGELOG.md -s",
    "bump-version": "npm --no-git-tag-version version",
    "prepare-release": "cross-env npm run changelog && npm run build && npm run doc",
    "release": "standard-version",
    "sass-build-task:scss-compile:bootstrap": "node-sass src/aurelia-slickgrid/styles/slickgrid-theme-bootstrap.scss -o dist/styles/css --output-style compressed",
    "postsass-build-task:scss-compile:bootstrap": "postcss --no-map --use autoprefixer --output dist/styles/css/slickgrid-theme-bootstrap.css dist/styles/css/slickgrid-theme-bootstrap.css --output-style compressed",
    "sass-build-task:scss-compile:material": "node-sass src/aurelia-slickgrid/styles/slickgrid-theme-material.scss -o dist/styles/css --output-style compressed",
    "postsass-build-task:scss-compile:material": "postcss --no-map --use autoprefixer --output dist/styles/css/slickgrid-theme-material.css dist/styles/css/slickgrid-theme-material.css --output-style compressed",
    "sass-build-task:scss-compile:material-bare": "node-sass src/aurelia-slickgrid/styles/slickgrid-theme-material.bare.scss -o dist/styles/css --output-style compressed",
    "postsass-build-task:scss-compile:material-bare": "postcss --no-map --use autoprefixer --output dist/styles/css/slickgrid-theme-material.bare.css dist/styles/css/slickgrid-theme-material.bare.css --output-style compressed",
    "sass-build-task:scss-compile:material-lite": "node-sass src/aurelia-slickgrid/styles/slickgrid-theme-material.lite.scss -o dist/styles/css --output-style compressed",
    "postsass-build-task:scss-compile:material-lite": "postcss --no-map --use autoprefixer --output dist/styles/css/slickgrid-theme-material.lite.css dist/styles/css/slickgrid-theme-material.lite.css --output-style compressed",
    "sass-build-task:scss-compile:salesforce": "node-sass src/aurelia-slickgrid/styles/slickgrid-theme-salesforce.scss -o dist/styles/css --output-style compressed",
    "postsass-build-task:scss-compile:salesforce": "postcss --no-map --use autoprefixer --output dist/styles/css/slickgrid-theme-salesforce.css dist/styles/css/slickgrid-theme-salesforce.css --output-style compressed",
    "sass-build-task:scss-compile:salesforce-bare": "node-sass src/aurelia-slickgrid/styles/slickgrid-theme-salesforce.bare.scss -o dist/styles/css --output-style compressed",
    "postsass-build-task:scss-compile:salesforce-bare": "postcss --no-map --use autoprefixer --output dist/styles/css/slickgrid-theme-salesforce.bare.css dist/styles/css/slickgrid-theme-salesforce.bare.css --output-style compressed",
    "sass-build-task:scss-compile:salesforce-lite": "node-sass src/aurelia-slickgrid/styles/slickgrid-theme-salesforce.lite.scss -o dist/styles/css --output-style compressed",
    "postsass-build-task:scss-compile:salesforce-lite": "postcss --no-map --use autoprefixer --output dist/styles/css/slickgrid-theme-salesforce.lite.css dist/styles/css/slickgrid-theme-salesforce.lite.css --output-style compressed",
    "sass:build": "run-p sass-build-task:scss-compile:*",
    "presass:copy": "cross-env copyfiles -f src/aurelia-slickgrid/styles/*.png dist/styles/css",
    "sass:copy": "cross-env copyfiles -f src/aurelia-slickgrid/styles/*.scss dist/styles/sass",
    "postsass:copy": "cross-env copyfiles -f src/aurelia-slickgrid/styles/*.png dist/styles/sass",
    "presass:watch:bootstrap": "cross-env npm run sass-build-task:scss-compile:bootstrap",
    "sass:watch:bootstrap": "cross-env npm run sass-build-task:scss-compile:bootstrap -- -watch",
    "presass:watch:material": "cross-env npm run sass-build-task:scss-compile:material",
    "sass:watch:material": "cross-env npm run sass-build-task:scss-compile:material -- -watch",
    "presass:watch:salesforce": "cross-env npm run sass-build-task:scss-compile:salesforce",
    "sass:watch:salesforce": "cross-env npm run sass-build-task:scss-compile:salesforce -- -watch",
    "sass:watch": "run-p sass:watch:bootstrap sass:watch:material sass:watch:salesforce sass:copy:watch",
    "sass:copy:watch": "nodemon --ext scss --watch src/aurelia-slickgrid/styles/*.scss --exec \"npm run sass:copy\"",
    "start:dev": "webpack-dev-server -d --port=9000 --env.development --config webpack.config.js",
    "start:webpack:dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server -d --port=9000 --env.server --config webpack.config.js",
    "cypress": "cd test/cypress && node node_modules/cypress/bin/cypress open",
    "cypress:ci": "cd test/cypress && npm run cypress:ci",
    "test:jest": "npx jest --watch --config test/jest.config.js",
    "test:jest:coverage": "npx jest --runInBand --coverage --config test/jest.config.js",
    "build:demo": "webpack --progress -p --env.production --env.extractCss",
    "serve:demo": "http-server ./docs -p 9000 -a localhost"
  },
  "dependencies": {
    "aurelia-event-aggregator": "^1.0.3",
    "aurelia-framework": "^1.3.1",
    "aurelia-i18n": "^2.3.2",
    "aurelia-pal": "^1.8.2",
    "aurelia-templating-resources": "^1.13.1",
    "dompurify": "^2.2.2",
    "excel-builder-webpacker": "^2.0.1",
    "flatpickr": "^4.6.6",
    "jquery": "^3.5.1",
    "jquery-ui-dist": "^1.12.1",
    "lodash.isequal": "^4.5.0",
    "moment-mini": "^2.24.0",
    "slickgrid": "^2.4.32",
    "text-encoding-utf-8": "^1.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/dompurify": "^2.0.4",
    "@types/flatpickr": "^3.1.2",
    "@types/i18next": "^13.0.0",
    "@types/i18next-xhr-backend": "^1.4.2",
    "@types/jest": "^26.0.15",
    "@types/jquery": "^3.5.4",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "@types/text-encoding-utf-8": "^1.0.1",
    "@types/webpack": "^4.41.24",
    "app-settings-loader": "^1.0.6",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-cli": "^2.0.2",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-http-client": "^1.3.1",
    "aurelia-loader-nodejs": "^1.1.0",
    "aurelia-pal-nodejs": "^2.0.0",
    "aurelia-polyfills": "latest",
    "aurelia-router": "^1.7.1",
    "aurelia-testing": "^1.0.0",
    "aurelia-webpack-plugin": "^4.0.0",
    "autoprefixer": "^10.0.1",
    "bluebird": "^3.7.2",
    "bootstrap": "^3.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.8.0",
    "concurrently": "^5.3.0",
    "conventional-changelog-cli": "^2.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "copyfiles": "^2.4.0",
    "cross-env": "^7.0.2",
    "css-loader": "5.0.0",
    "del": "^6.0.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.12.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "expose-loader": "^1.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.2.0",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-util": "^3.0.8",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "http-server": "^0.12.3",
    "i18next-xhr-backend": ">=1.5.0",
    "isomorphic-fetch": "^3.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^26.6.2",
    "jest-cli": "^26.6.2",
    "jest-environment-jsdom": "^26.6.2",
    "jest-extended": "^0.11.5",
    "jest-junit": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^1.2.1",
    "minimatch": "^3.0.4",
    "node-sass": "5.0.0",
    "npm-run-all": "^4.1.5",
    "opn": "^6.0.0",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.2.0",
    "promise-polyfill": "^8.2.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.5",
    "standard-version": "^9.0.0",
    "style-loader": "2.0.0",
    "through2": "^4.0.2",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5",
    "uglify-js": "^3.11.4",
    "url-loader": "4.1.1",
    "vinyl-fs": "^3.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "aurelia": {
    "build": {
      "resources": [
        "aurelia-slickgrid/aurelia-slickgrid",
        "aurelia-slickgrid/slick-pagination"
      ]
    }
  },
  "jspm": {
    "registry": "npm",
    "jspmPackage": true,
    "main": "index",
    "format": "amd",
    "directories": {
      "lib": "dist/amd"
    },
    "dependencies": {
      "aurelia-event-aggregator": ">=1.0.1",
      "aurelia-framework": ">=1.1.0",
      "aurelia-i18n": ">=2.1.0",
      "aurelia-pal": ">=1.4.0",
      "dompurify": "^2.0.12",
      "excel-builder-webpacker": "^1.0.6",
      "flatpickr": "4.6.3",
      "i18next-xhr-backend": ">=1.5.0",
      "jquery": "^3.4.1",
      "jquery-ui-dist": "^1.12.1",
      "lodash.isequal": "^4.5.0",
      "moment-mini": "2.24.0",
      "slickgrid": "^2.4.27",
      "text-encoding-utf-8": "^1.0.2"
    }
  },
  "standard-version": {
    "skip": {
      "bump": false,
      "changelog": false,
      "commit": false,
      "tag": false
    }
  }
}
