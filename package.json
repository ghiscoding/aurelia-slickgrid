{
  "name": "aurelia-slickgrid",
  "version": "3.12.0",
  "description": "Slickgrid components made available in Aurelia",
  "keywords": [
    "aurelia",
    "datagrid",
    "datatable",
    "plugin",
    "slickgrid"
  ],
  "homepage": "https://github.com/ghiscoding/aurelia-slickgrid",
  "bugs": {
    "url": "https://github.com/ghiscoding/aurelia-slickgrid/issues"
  },
  "license": "MIT",
  "author": "Ghislain B.",
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/ghiscoding/aurelia-slickgrid"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "delete:dist": "cross-env rimraf dist",
    "lint": "eslint src/aurelia-slickgrid/** --ext .ts --quiet",
    "build:amd": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/amd --module amd",
    "postbuild:amd": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/amd",
    "build:commonjs": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/commonjs --module commonjs",
    "postbuild:commonjs": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/commonjs",
    "build:esm": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/esm --module esnext --target es2018",
    "postbuild:esm": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/esm",
    "build:native-modules": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/native-modules --module es2015",
    "postbuild:native-modules": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/native-modules",
    "prebuild": "run-p delete:dist lint",
    "build": "run-p build:amd build:commonjs build:esm build:native-modules",
    "postbuild": "npm-run-all copy-i18n:dist copy-asset-lib",
    "build:with-e2e": "npm-run-all build cypress:ci",
    "copy-asset-lib": "cross-env copyfiles --up 2 src/assets/lib/** dist",
    "copy-i18n:dist": "cross-env copyfiles --up 3 src/assets/i18n/**/*.* dist/i18n",
    "predoc": "cross-env rimraf api.json && rimraf dist/doc-temp && tsc --project src/aurelia-slickgrid/tsconfig.build.json --outFile dist/doc-temp/aurelia-validation.js && node shape-defs && copyfiles tsconfig.json dist/doc-temp",
    "doc": "cross-env typedoc --json api.json --excludeExternals --includeDeclarations --mode modules --target ES6 --name aurelia-validation-docs dist/doc-temp/",
    "postdoc": "cross-env node shape-doc && rimraf dist/doc-temp",
    "changelog": "cross-env conventional-changelog -p angular -i CHANGELOG.md -s",
    "bump-version": "npm --no-git-tag-version version",
    "prepare-release": "cross-env npm run changelog && npm run build && npm run doc",
    "release": "standard-version",
    "start:dev": "webpack serve --env development",
    "cypress": "cd test/cypress && node node_modules/cypress/bin/cypress open",
    "cypress:ci": "cd test/cypress && npm run cypress:ci",
    "test:ci": "npx jest --runInBand --coverage=true --ci --config ./test/jest.config.js",
    "test:jest": "npx jest --watch --detectOpenHandles --config test/jest.config.js",
    "test:jest:coverage": "npx jest --detectOpenHandles --runInBand --coverage --config test/jest.config.js",
    "build:demo": "webpack --env production"
  },
  "dependencies": {
    "@slickgrid-universal/common": "^0.19.0",
    "@slickgrid-universal/custom-footer-component": "^0.19.0",
    "@slickgrid-universal/empty-warning-component": "^0.19.0",
    "@slickgrid-universal/event-pub-sub": "^0.19.0",
    "@slickgrid-universal/pagination-component": "^0.19.0",
    "aurelia-event-aggregator": "^1.0.3",
    "aurelia-framework": "^1.3.1",
    "aurelia-i18n": "^3.1.4",
    "aurelia-pal": "^1.8.2",
    "aurelia-templating-resources": "^1.13.1",
    "dompurify": "^2.3.3",
    "jquery": "~3.5.1",
    "typescript": ">=4.4.3"
  },
  "peerDependencies": {
    "i18next": ">= 14.1.1"
  },
  "devDependencies": {
    "@popperjs/core": "^2.10.2",
    "@slickgrid-universal/composite-editor-component": "^0.19.0",
    "@slickgrid-universal/custom-tooltip-plugin": "^0.19.0",
    "@slickgrid-universal/excel-export": "^0.19.0",
    "@slickgrid-universal/graphql": "^0.19.0",
    "@slickgrid-universal/odata": "^0.19.0",
    "@slickgrid-universal/rxjs-observable": "^0.19.0",
    "@slickgrid-universal/text-export": "^0.19.0",
    "@types/bluebird": "^3.5.36",
    "@types/dompurify": "^2.3.1",
    "@types/i18next-xhr-backend": "^1.4.2",
    "@types/jest": "^27.0.2",
    "@types/jquery": "^3.5.8",
    "@types/node": "^16.11.6",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@webpack-cli/serve": "^1.6.0",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-cli": "^2.0.3",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-http-client": "^1.3.1",
    "aurelia-loader-nodejs": "^1.1.0",
    "aurelia-pal-nodejs": "^2.0.0",
    "aurelia-polyfills": "latest",
    "aurelia-router": "^1.7.1",
    "aurelia-testing": "^1.0.0",
    "aurelia-webpack-plugin": "^5.0.2",
    "bootstrap": "^5.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "codecov": "^3.8.3",
    "concurrently": "^6.3.0",
    "conventional-changelog-cli": "^2.1.1",
    "copy-webpack-plugin": "^9.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "6.5.0",
    "del": "^6.0.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^8.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.2.0",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.5.0",
    "i18next-xhr-backend": "^3.2.2",
    "isomorphic-fetch": "^3.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^27.3.1",
    "jest-cli": "^27.3.1",
    "jest-environment-jsdom": "^27.3.1",
    "jest-extended": "^1.1.0",
    "jest-junit": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "~2.3.0",
    "minimatch": "^3.0.4",
    "moment-mini": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "opn": "^6.0.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "promise-polyfill": "^8.2.1",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "standard-version": "^9.3.2",
    "style-loader": "3.3.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.7",
    "vinyl-fs": "^3.0.3",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.8"
  },
  "aurelia": {
    "build": {
      "resources": [
        "aurelia-slickgrid/aurelia-slickgrid"
      ]
    }
  },
  "standard-version": {
    "skip": {
      "bump": false,
      "changelog": false,
      "commit": false,
      "tag": false
    }
  }
}
