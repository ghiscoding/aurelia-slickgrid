{
  "name": "aurelia-slickgrid",
  "version": "2.18.2",
  "description": "Slickgrid components made available in Aurelia",
  "keywords": [
    "aurelia",
    "datagrid",
    "datatable",
    "plugin",
    "slickgrid"
  ],
  "homepage": "https://github.com/ghiscoding/aurelia-slickgrid",
  "bugs": {
    "url": "https://github.com/ghiscoding/aurelia-slickgrid/issues"
  },
  "license": "MIT",
  "author": "Ghislain B.",
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/ghiscoding/aurelia-slickgrid"
  },
  "scripts": {
    "lint": "cross-env tslint --project tsconfig.json",
    "delete:dist": "cross-env rimraf dist",
    "build:amd": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/amd --module amd",
    "postbuild:amd": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/amd",
    "build:commonjs": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/commonjs --module commonjs",
    "postbuild:commonjs": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/commonjs",
    "build:es2017": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/es2017 --module es2015 --target es2017",
    "postbuild:es2017": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/es2017",
    "build:es2015": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/es2015 --module es2015 --target es2015",
    "postbuild:es2015": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/es2015",
    "build:native-modules": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/native-modules --module es2015",
    "postbuild:native-modules": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/native-modules",
    "build:system": "cross-env tsc --project src/aurelia-slickgrid/tsconfig.build.json --outDir dist/system --module system",
    "postbuild:system": "cross-env copyfiles --up 2 src/aurelia-slickgrid/**/*.html dist/system",
    "prebuild": "npm-run-all delete:dist test:jest",
    "build": "npm-run-all build:amd build:commonjs build:es2015 build:es2017 build:native-modules build:system",
    "postbuild": "npm-run-all sass:build sass:copy copy-i18n:dist copy-asset-lib",
    "build:with-e2e": "npm-run-all build cypress:ci:mochawesome",
    "copy-asset-lib": "cross-env copyfiles --up 2 src/assets/lib/** dist",
    "copy-i18n:dist": "cross-env copyfiles --up 3 src/assets/i18n/**/*.* dist/i18n",
    "predoc": "cross-env rimraf api.json && rimraf dist/doc-temp && tsc --project src/aurelia-slickgrid/tsconfig.build.json --outFile dist/doc-temp/aurelia-validation.js && node shape-defs && copyfiles tsconfig.json dist/doc-temp",
    "doc": "cross-env typedoc --json api.json --excludeExternals --includeDeclarations --mode modules --target ES6 --name aurelia-validation-docs dist/doc-temp/",
    "postdoc": "cross-env node shape-doc && rimraf dist/doc-temp",
    "changelog": "cross-env conventional-changelog -p angular -i CHANGELOG.md -s",
    "bump-version": "npm --no-git-tag-version version",
    "prepare-release": "cross-env npm run changelog && npm run build && npm run doc",
    "release": "standard-version",
    "sass-build-task:scss-compile:bootstrap": "node-sass src/aurelia-slickgrid/styles/slickgrid-theme-bootstrap.scss -o dist/styles/css --output-style compressed",
    "postsass-build-task:scss-compile:bootstrap": "postcss --use autoprefixer --output dist/styles/css/slickgrid-theme-bootstrap.css dist/styles/css/slickgrid-theme-bootstrap.css --output-style compressed",
    "sass-build-task:scss-compile:material": "node-sass src/aurelia-slickgrid/styles/slickgrid-theme-material.scss -o dist/styles/css --output-style compressed",
    "postsass-build-task:scss-compile:material": "postcss --use autoprefixer --output dist/styles/css/slickgrid-theme-material.css dist/styles/css/slickgrid-theme-material.css --output-style compressed",
    "sass-build-task:scss-compile:salesforce": "node-sass src/aurelia-slickgrid/styles/slickgrid-theme-salesforce.scss -o dist/styles/css --output-style compressed",
    "postsass-build-task:scss-compile:salesforce": "postcss --use autoprefixer --output dist/styles/css/slickgrid-theme-salesforce.css dist/styles/css/slickgrid-theme-salesforce.css --output-style compressed",
    "sass:build": "run-p sass-build-task:scss-compile:*",
    "sass:copy": "cross-env copyfiles -f src/aurelia-slickgrid/styles/*.scss dist/styles/sass",
    "presass:watch:bootstrap": "cross-env npm run sass-build-task:scss-compile:bootstrap",
    "sass:watch:bootstrap": "cross-env npm run sass-build-task:scss-compile:bootstrap -- -watch",
    "presass:watch:material": "cross-env npm run sass-build-task:scss-compile:material",
    "sass:watch:material": "cross-env npm run sass-build-task:scss-compile:material -- -watch",
    "presass:watch:salesforce": "cross-env npm run sass-build-task:scss-compile:salesforce",
    "sass:watch:salesforce": "cross-env npm run sass-build-task:scss-compile:salesforce -- -watch",
    "sass:watch": "run-p sass:watch:bootstrap sass:watch:material sass:watch:salesforce sass:copy:watch",
    "sass:copy:watch": "nodemon --ext scss --watch src/aurelia-slickgrid/styles/*.scss --exec \"npm run sass:copy\"",
    "serve": "http-server dist-demo -p 9000 -a localhost",
    "start": "nps",
    "start:dev": "nps",
    "start:webpack:dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server -d --port=9000 --devtool '#source-map' --inline --env.server --config webpack.config.js",
    "e2e:ci": "nps e2e.ci",
    "cypress:ci:mochawesome": "cd test/cypress && npm run cypress:ci:mochawesome",
    "cypress:ci:xml": "cd test/cypress && node node_modules/cypress/bin/cypress run --reporter xunit --reporter-options output=testresult.xml",
    "cypress:open": "cd test/cypress && node node_modules/cypress/bin/cypress open",
    "test:jest": "node_modules/.bin/jest --runInBand --coverage --config test/jest.config.js"
  },
  "dependencies": {
    "aurelia-event-aggregator": "^1.0.3",
    "aurelia-framework": "^1.3.1",
    "aurelia-i18n": "^2.3.2",
    "aurelia-pal": "^1.8.2",
    "aurelia-templating-resources": "^1.13.0",
    "dompurify": "^2.0.11",
    "excel-builder-webpacker": "^1.0.5",
    "flatpickr": ">=4.5.0",
    "jquery": "^3.4.1",
    "jquery-ui-dist": "^1.12.1",
    "lodash.isequal": "^4.5.0",
    "moment-mini": "^2.24.0",
    "slickgrid": "^2.4.24",
    "text-encoding-utf-8": "^1.0.2"
  },
  "devDependencies": {
    "@types/dompurify": "^2.0.2",
    "@types/flatpickr": "^3.1.2",
    "@types/i18next": "^13.0.0",
    "@types/i18next-xhr-backend": "^1.4.2",
    "@types/jest": "^25.2.3",
    "@types/jquery": "^3.3.38",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.0.1",
    "@types/text-encoding-utf-8": "^1.0.1",
    "@types/webpack": "^3.8.18",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-cli": "^1.2.3",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-http-client": "^1.3.1",
    "aurelia-loader-nodejs": "^1.1.0",
    "aurelia-pal-nodejs": "^1.2.0",
    "aurelia-polyfills": "latest",
    "aurelia-router": "^1.7.1",
    "aurelia-testing": "^1.0.0",
    "aurelia-webpack-plugin": "^2.0.0-rc.5",
    "autoprefixer": "^9.7.6",
    "awesome-typescript-loader": "^3.5.0",
    "bluebird": "^3.7.2",
    "bootstrap": "^3.4.1",
    "codecov": "^3.6.5",
    "concurrently": "^5.2.0",
    "conventional-changelog-cli": "^2.0.31",
    "copy-webpack-plugin": "4.0.1",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "0.28.4",
    "del": "^5.1.0",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "font-awesome": "^4.7.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-util": "^3.0.8",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.30.1",
    "http-server": "^0.12.1",
    "i18next-xhr-backend": ">=1.5.0",
    "isomorphic-fetch": "^2.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "jest-environment-jsdom": "^26.0.1",
    "jest-extended": "^0.11.5",
    "jest-junit": "^10.0.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "0.5.7",
    "minimatch": "^3.0.4",
    "node-sass": "4.14.1",
    "npm-run-all": "^4.1.5",
    "nps": "^5.9.12",
    "nps-utils": "1.7.0",
    "opn": "^6.0.0",
    "postcss-cli": "^7.1.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^6.0.6",
    "standard-version": "^8.0.0",
    "style-loader": "0.18.2",
    "through2": "^3.0.1",
    "ts-jest": "^26.0.0",
    "ts-loader": "^5.2.1",
    "ts-node": "^8.10.1",
    "tslint": "^6.0.0",
    "typedoc": "^0.17.6",
    "typescript": "^3.9.3",
    "uglify-js": "^3.9.3",
    "url-loader": "0.5.8",
    "vinyl-fs": "^3.0.3",
    "wait-on": "5.0.0",
    "webpack": "3.5.5",
    "webpack-dev-server": "^2.11.2"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "aurelia": {
    "build": {
      "resources": [
        "aurelia-slickgrid/aurelia-slickgrid",
        "aurelia-slickgrid/slick-pagination"
      ]
    }
  },
  "jspm": {
    "registry": "npm",
    "jspmPackage": true,
    "main": "index",
    "format": "amd",
    "directories": {
      "lib": "dist/amd"
    },
    "dependencies": {
      "aurelia-event-aggregator": ">=1.0.1",
      "aurelia-framework": ">=1.1.0",
      "aurelia-i18n": ">=2.1.0",
      "aurelia-pal": ">=1.4.0",
      "dompurify": "^2.0.11",
      "excel-builder-webpacker": "^1.0.6",
      "flatpickr": ">=4.5.0",
      "i18next-xhr-backend": ">=1.5.0",
      "jquery": "^3.4.1",
      "jquery-ui-dist": "^1.12.1",
      "lodash.isequal": "^4.5.0",
      "moment-mini": "2.24.0",
      "slickgrid": "^2.4.22",
      "text-encoding-utf-8": "^1.0.2"
    }
  },
  "standard-version": {
    "skip": {
      "bump": false,
      "changelog": false,
      "commit": false,
      "tag": false
    }
  }
}
