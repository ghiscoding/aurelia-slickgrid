/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0__models_fieldType__ = __webpack_require__(/*! ../models/fieldType */ 26);

var mapDateFormatByFieldType = function (fieldType) {
    switch (fieldType) {
        case __WEBPACK_IMPORTED_MODULE_0__models_fieldType__["a" /* FieldType */].dateUs:
            return 'M/D/YYYY';
        case __WEBPACK_IMPORTED_MODULE_0__models_fieldType__["a" /* FieldType */].dateTimeUs:
            return 'M/D/YYYY h:m:s';
        case __WEBPACK_IMPORTED_MODULE_0__models_fieldType__["a" /* FieldType */].dateUsShort:
            return 'M/D/YY';
        case __WEBPACK_IMPORTED_MODULE_0__models_fieldType__["a" /* FieldType */].dateTimeUsShort:
            return 'M/D/YY h:m:s';
        case __WEBPACK_IMPORTED_MODULE_0__models_fieldType__["a" /* FieldType */].dateTimeIso:
            return 'YYYY-MM-DD h:m:s';
        case __WEBPACK_IMPORTED_MODULE_0__models_fieldType__["a" /* FieldType */].dateIso:
        default:
            return 'YYYY-MM-DD';
    }
};
var testFilterCondition = function (operator, value1, value2) {
    switch (operator) {
        case '<': return (value1 < value2);
        case '<=': return (value1 <= value2);
        case '>': return (value1 > value2);
        case '>=': return (value1 >= value2);
        case '!=':
        case '<>': return (value1 !== value2);
        case '=':
        case '==': return (value1 === value2);
    }
    return true;
};


