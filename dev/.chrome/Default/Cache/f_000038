      };

      xhr.onabort = function (e) {
        reject(new HttpResponseMessage(requestMessage, {
          response: e,
          status: xhr.status,
          statusText: xhr.statusText
        }, 'abort'));
      };
    });

    return Promise.resolve(requestMessage).then(function (message) {
      var processRequest = function processRequest() {
