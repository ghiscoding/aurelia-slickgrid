/***/ 289:
/*!*******************************************************!*\
  !*** ./src/aurelia-slickgrid/sorters/dateUsSorter.ts ***!
  \*******************************************************/
/*! exports provided: dateUsSorter */
/*! exports used: dateUsSorter */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "a", function() { return dateUsSorter; });
/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0_moment__ = __webpack_require__(/*! moment */ 0);
/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0_moment___default = __webpack_require__.n(__WEBPACK_IMPORTED_MODULE_0_moment__);

var DATE_FORMAT = 'M/D/YYYY';
var dateUsSorter = function (value1, value2, sortDirection) {
    if (!__WEBPACK_IMPORTED_MODULE_0_moment__(value1, DATE_FORMAT, true).isValid() || !__WEBPACK_IMPORTED_MODULE_0_moment__(value2, DATE_FORMAT, true).isValid()) {
        return 0;
    }
    var date1 = __WEBPACK_IMPORTED_MODULE_0_moment__(value1, DATE_FORMAT, true);
    var date2 = __WEBPACK_IMPORTED_MODULE_0_moment__(value2, DATE_FORMAT, true);
    var diff = parseInt(date1.format('X'), 10) - parseInt(date2.format('X'), 10);
    return sortDirection * (diff === 0 ? 0 : (diff > 0 ? 1 : -1));
};


