/***/ 303:
/*!**********************************************************************!*\
  !*** ./src/aurelia-slickgrid/formatters/percentCompleteFormatter.ts ***!
  \**********************************************************************/
/*! exports provided: percentCompleteFormatter */
/*! exports used: percentCompleteFormatter */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "a", function() { return percentCompleteFormatter; });
var percentCompleteFormatter = function (row, cell, value, columnDef, dataContext) {
    if (value === null || value === '') {
        return '-';
    }
    else if (value < 50) {
        return "<span style='color:red;font-weight:bold;'>" + value + "%</span>";
    }
    else {
        return "<span style='color:green'>" + value + "%</span>";
    }
};


