{
  "name": "aurelia-slickgrid",
  "version": "7.0.2",
  "description": "Slickgrid components made available in Aurelia",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "development": "./dist/index.dev.mjs"
  },
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "keywords": [
    "aurelia",
    "datagrid",
    "datatable",
    "plugin",
    "slickgrid"
  ],
  "homepage": "https://github.com/ghiscoding/aurelia-slickgrid",
  "bugs": {
    "url": "https://github.com/ghiscoding/aurelia-slickgrid/issues"
  },
  "license": "MIT",
  "author": "Ghislain B.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ghiscoding/aurelia-slickgrid.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build1": "webpack --env production",
    "build": "tsc && vite build",
    "analyze": "webpack --env production --analyze"
  },
  "comments": {
    "new-release": "npm run release, note that yarn is not supported with release-it and will throw an error"
  },
  "peerDependencies": {
    "@aurelia/i18n": "latest",
    "@aurelia/kernel": "latest",
    "@aurelia/platform": "latest",
    "@aurelia/platform-browser": "latest",
    "@aurelia/router-lite": "latest",
    "@aurelia/runtime": "latest",
    "@aurelia/runtime-html": "latest",
    "@aurelia/validation-html": "latest",
    "@slickgrid-universal/common": "~4.0.3",
    "@slickgrid-universal/custom-footer-component": "~4.0.3",
    "@slickgrid-universal/empty-warning-component": "~4.0.3",
    "@slickgrid-universal/event-pub-sub": "~4.0.2",
    "@slickgrid-universal/pagination-component": "~4.0.3",
    "@slickgrid-universal/row-detail-view-plugin": "~4.0.3",
    "aurelia": "latest",
    "dompurify": "^3.0.6",
    "sortablejs": "^1.15.1"
  },
  "dependencies": {
  }
}
